{
    "name": "salehhub/authx-auth",
    "description": "Reusable AuthX authentication and admin allowlist middleware for Laravel apps.",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^8.4",
        "illuminate/support": "^12.0",
        "illuminate/http": "^12.0",
        "illuminate/routing": "^12.0",
        "illuminate/auth": "^12.0",
        "illuminate/contracts": "^12.0",
        "laravel/socialite": "^5.24"
    },
    "autoload": {
        "psr-4": {
            "AuthxAuth\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "AuthxAuth\\AuthxAuthServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
